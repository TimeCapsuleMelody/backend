{
          "[python]": {
                    "editor.defaultFormatter": "ms-python.python"
          },
          "editor.formatOnSave": true,
          "python.formatting.autopep8Args": [
                    "--max-line-length=120",
                    "--aggressive"
          ],
          "editor.codeActionsOnSave": [
                    "source.organizeImports"
          ]
}